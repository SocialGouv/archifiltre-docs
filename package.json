{
  "name": "cheapExp",
  "version": "1.0.0",
  "description": "Explorer plus plus for archive",
  "main": "electron/index.js",
  "scripts": {
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src mocha --require babel-register",
    "buildDev": "webpack --progress --mode development",
    "buildProd": "webpack --progress --mode production",
    "devServer": "webpack-dev-server --mode development",
    "watchBuildDev": "webpack --watch --progress --mode development",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "windows": "electron-builder --windows portable --x64",
    "mac": "electron-builder --mac dmg",
    "postinstall": "electron-builder install-app-deps package.json",
    "electron": "electron ./electron",
    "start": "",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "document-wip": "jsdoc2md ./src/*.js > ./docs/doc.md"
  },
  "build": {
    "appId": "io.github.archifiltre",
    "files": [
      "electron/index.js",
      "electron/dist",
      "electron/build/icon.*",
      "package.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeanbaptisteassouad/cheapExp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeanbaptisteassouad/cheapExp/issues"
  },
  "homepage": "https://github.com/jeanbaptisteassouad/cheapExp#readme",
  "devDependencies": {
    "@socialgouv/eslint-config-react": "^0.6.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.3",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chai-exclude": "1.0.8",
    "colors": "1.2.1",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.10",
    "electron": "^5.0.1",
    "electron-builder": "^20.39.0",
    "electron-react-devtools": "^0.5.3",
    "eslint": "^5.16.0",
    "file-loader": "2.0.0",
    "flow-bin": "0.66.0",
    "flow-webpack-plugin": "1.2.0",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jsdoc-to-markdown": "5.0.0",
    "mocha": "5.0.5",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.20.3",
    "svg-url-loader": "^2.3.2",
    "url-loader": "^1.1.2",
    "webpack": "4.1.0",
    "webpack-cli": "2.0.4",
    "webpack-dev-server": "3.1.1",
    "worker-loader": "1.1.1"
  },
  "dependencies": {
    "dateformat": "3.0.3",
    "electron-ga": "1.0.6",
    "file-saver": "1.3.3",
    "foundation-sites": "^6.5.3",
    "immutable": "4.0.0-rc.9",
    "jquery": "^3.4.1",
    "js-sha512": "0.8.0",
    "jszip": "3.1.5",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "riek": "1.1.0",
    "tar-fs": "1.16.3",
    "tar-stream": "1.6.2",
    "what-input": "^5.2.1",
    "workbox-webpack-plugin": "3.1.0",
    "xml": "1.0.1"
  }
