{
  "name": "archifiltre",
  "version": "2.2.0",
  "description": "Explorer plus plus for archive",
  "author": "",
  "license": "ISC",
  "main": "electron/dist/main.js",
  "scripts": {
    "dev": "concurrently -k \"yarn dev-main\" \"yarn dev-renderer\"",
    "dev-main": "webpack --mode development --config webpack.electron-main.js --watch",
    "dev-renderer": "webpack-dev-server --mode development",
    "dev-test": "concurrently -k \"yarn dev-main\" \"yarn dev-renderer --autoload ./test-folder\"",
    "dev-app": "cross-env DEV_SERVER=true electron ./electron/dist/main",
    "test": "jest \"src/.*\\.test\\.[jt]s\"",
    "e2e": "jest \"e2e/.*\\\\.test\\\\.[jt]s\"",
    "prepare-prod": "webpack --progress --mode production --config webpack.electron-main.js && webpack --progress --mode production",
    "build-prod": "yarn prepare-prod && yarn win32 && yarn win64 && yarn linux && yarn mac",
    "win32": "electron-builder --windows portable --ia32",
    "win64": "electron-builder --windows portable --x64",
    "mac": "electron-builder --mac dmg",
    "linux": "electron-builder --linux AppImage",
    "lint": "eslint . && tslint -p .",
    "lint-fix": "eslint --fix .",
    "postinstall": "electron-builder install-app-deps",
    "analyze-bundle": "webpack --profile --json > stats.json && sed -i -e 1,4d stats.json && webpack-bundle-analyzer ./stats.json"
  },
  "build": {
    "appId": "io.github.archifiltre",
    "files": [
      "electron/dist",
      "electron/build/icon.*",
      "package.json"
    ],
    "directories": {
      "output": "dist/${os}-${arch}"
    },
    "mac": {
      "icon": "./electron/build/icon.icns"
    },
    "win": {
      "icon": "./electron/build/icon.png"
    },
    "linux": {
      "category": "Office",
      "icon": "./electron/build/icon.png"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --list-different"
    ],
    "*.ts": [
      "tslint --fix",
      "prettier --list-different"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/archifiltre"
  },
  "bugs": {
    "url": "https://github.com/SocialGouv/archifiltre/issues"
  },
  "homepage": "https://github.com/SocialGouv/archifiltre#readme",
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@socialgouv/eslint-config-react": "0.29.0",
    "@socialgouv/tslint-config-recommended": "0.29.0",
    "@types/jest": "26.0.0",
    "@types/lodash": "4.14.155",
    "@types/node": "12",
    "@types/react": "16.9.38",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/redux-mock-store": "1.0.2",
    "@types/styled-components": "^5.1.0",
    "awesome-typescript-loader": "5.2.1",
    "concurrently": "5.2.0",
    "copy-webpack-plugin": "6.0.2",
    "cross-env": "7.0.2",
    "css-loader": "3.6.0",
    "deep-equal": "2.0.3",
    "dotenv": "8.2.0",
    "electron": "9.0.4",
    "electron-builder": "22.7.0",
    "electron-react-devtools": "0.5.3",
    "eslint": "7.2.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "fast-check": "1.25.1",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jest-date-mock": "1.0.8",
    "lint-staged": "10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "2.0.5",
    "rmfr": "2.0.0",
    "sass-loader": "8.0.2",
    "source-map-loader": "1.0.0",
    "spectron": "11.0.0",
    "style-loader": "1.2.1",
    "svg-url-loader": "6.0.0",
    "ts-jest": "26.1.0",
    "tslint": "6.1.2",
    "typescript": "3.9.5",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@devexpress/dx-react-chart": "^2.6.3",
    "@devexpress/dx-react-chart-material-ui": "^2.6.3",
    "@devexpress/dx-react-core": "^2.6.3",
    "@material-ui/core": "^4.10.2",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@sentry/browser": "^5.17.0",
    "@types/pizzip": "^3.0.1",
    "angular-expressions": "^1.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "csv-parse": "^4.10.1",
    "dateformat": "3.0.3",
    "docxtemplater": "3.17.9",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "fast-memoize": "^2.5.2",
    "file-saver": "2.0.2",
    "i18next": "^19.4.5",
    "immutable": "4.0.0-rc.12",
    "js-md5": "0.7.3",
    "jszip": "3.5.0",
    "lodash": "^4.17.15",
    "md5-file": "^5.0.0",
    "mime-types": "^2.1.27",
    "pizzip": "^3.0.6",
    "raven": "^2.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-icons": "^3.10.0",
    "react-notifications": "^1.6.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.5",
    "styled-components": "^5.1.1",
    "triple-beam": "^1.3.0",
    "uuid": "8.1.0",
    "webpack-fork-loader": "^1.0.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-sentry-raven-transport": "^1.2.0",
