{
  "name": "Archifiltre",
  "version": "3.1.0",
  "description": "Visualisez et améliorez vos arborescences de fichiers !",
  "author": "Fabrique des ministère sociaux",
  "license": "Apache-2.0",
  "main": "electron/dist/main.js",
  "scripts": {
    "dev": "concurrently -k \"yarn dev-main\" \"yarn dev-renderer\"",
    "dev-main": "webpack --mode development --config webpack.electron-main.js --watch",
    "dev-renderer": "webpack-dev-server --mode development",
    "dev-test": "concurrently -k \"yarn dev-main\" \"yarn dev-renderer --autoload ./test-folder\"",
    "dev-app": "cross-env DEV_SERVER=true electron ./electron/dist/main",
    "test": "jest \"src/.*\\.test\\.[jt]s\"",
    "test-coverage": "jest \"src/.*\\.test\\.[jt]s\" --coverage",
    "e2e": "jest \"e2e/.*\\\\.test\\\\.[jt]s\"",
    "prepare-prod": "webpack --progress --mode production --config webpack.electron-main.js && webpack --progress --mode production",
    "build-prod": "yarn prepare-prod && yarn win32 && yarn win64 && yarn linux && yarn mac",
    "win32": "electron-builder --windows portable --ia32",
    "win64": "electron-builder --windows portable --x64",
    "win32-msi": "electron-builder --windows portable --ia32 --windows=msi",
    "win64-msi": "electron-builder --windows portable --x64 --windows=msi",
    "mac": "electron-builder --mac dmg",
    "linux": "electron-builder --linux AppImage",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "postinstall": "electron-builder install-app-deps",
    "analyze-bundle": "webpack --profile --json > stats.json && sed -i -e 1,4d stats.json && webpack-bundle-analyzer ./stats.json",
    "generate-translation-csv": "node ./scripts/diff-translations.js",
    "import-translation-csv": "node ./scripts/translation-csv-to-json"
  },
  "build": {
    "appId": "io.github.archifiltre",
    "artifactName": "archifiltre-${version}.${ext}",
    "files": [
      "electron/dist",
      "electron/build/icon.*",
      "package.json"
    ],
    "directories": {
      "output": "dist/${os}-${arch}"
    },
    "mac": {
      "icon": "./electron/build/icon.icns"
    },
    "win": {
      "icon": "./electron/build/icon.png"
    },
    "linux": {
      "category": "Office",
      "icon": "./electron/build/icon.png"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.[t|j]s?(x)": [
      "eslint --fix",
      "prettier --list-different"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/archifiltre"
  },
  "bugs": {
    "url": "https://github.com/SocialGouv/archifiltre/issues"
  },
  "homepage": "https://github.com/SocialGouv/archifiltre#readme",
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@socialgouv/eslint-config-react": "1.37.0",
    "@types/jest": "26.0.14",
    "@types/lodash": "4.14.162",
    "@types/node": "12",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/redux-mock-store": "1.0.2",
    "@types/styled-components": "^5.1.4",
    "awesome-typescript-loader": "5.2.1",
    "concurrently": "5.3.0",
    "copy-webpack-plugin": "6.2.1",
    "cross-env": "7.0.2",
    "css-loader": "4.3.0",
    "csv-stringify": "^5.5.1",
    "deep-equal": "2.0.4",
    "dotenv": "8.2.0",
    "electron": "10.1.3",
    "electron-builder": "22.8.1",
    "electron-react-devtools": "0.5.3",
    "eslint": "7.10.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "fast-check": "2.4.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-date-mock": "1.0.8",
    "lint-staged": "10.4.0",
    "node-loader": "^1.0.2",
    "node-sass": "^4.14.1",
    "prettier": "2.1.2",
    "rmfr": "2.0.0",
    "sass-loader": "10.0.3",
    "source-map-loader": "1.1.1",
    "spectron": "12.0.0",
    "style-loader": "1.3.0",
    "svg-url-loader": "6.0.0",
    "ts-jest": "26.4.1",
    "typescript": "4.0.3",
    "url-loader": "4.1.1",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@devexpress/dx-react-chart": "^2.7.2",
    "@devexpress/dx-react-chart-material-ui": "^2.7.2",
    "@devexpress/dx-react-core": "^2.7.2",
    "@emeraldpay/hashicon-react": "^0.5.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@sentry/browser": "^5.24.2",
    "@types/pizzip": "^3.0.1",
    "angular-expressions": "^1.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "csv-parse": "^4.12.0",
    "dateformat": "3.0.3",
    "docxtemplater": "3.19.6",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "fast-memoize": "^2.5.2",
    "file-saver": "2.0.2",
    "fswin": "^3.19.908",
    "hidefile": "^3.0.0",
    "i18next": "^19.8.2",
    "js-md5": "0.7.3",
    "jszip": "3.5.0",
    "lodash": "^4.17.20",
    "md5-file": "^5.0.0",
    "mime-types": "^2.1.27",
    "pizzip": "^3.0.6",
    "raven": "^2.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.3",
    "react-icons": "^3.11.0",
    "react-notifications": "^1.7.2",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.6.3",
    "styled-components": "^5.2.0",
    "triple-beam": "^1.3.0",
    "uuid": "8.3.1",
    "webpack-fork-loader": "^1.0.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-sentry-raven-transport": "^1.2.0",
    "winston-transport": "^4.4.0",
    "xlsx": "^0.16.8",
    "xml": "^1.0.1",
    "yargs": "^16.0.3"
  }
}
