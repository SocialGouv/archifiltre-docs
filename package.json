{
  "name": "archifiltre",
  "version": "1.0.0",
  "description": "Explorer plus plus for archive",
  "main": "electron/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "yarn test --watch",
    "buildDev": "webpack --progress --mode development",
    "prepareProd": "webpack --progress --mode production",
    "buildProd": "yarn prepareProd && yarn win32 && yarn win64 && yarn mac && yarn linux",
    "devServer": "webpack-dev-server --mode development",
    "watchBuildDev": "webpack --watch --progress --mode development",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "win32": "electron-builder --windows portable --ia32",
    "win64": "electron-builder --windows portable --x64",
    "mac": "electron-builder --mac dmg",
    "linux": "electron-builder --linux AppImage",
    "postinstall": "electron-builder install-app-deps package.json",
    "electron": "electron ./electron",
    "electron-server": "cross-env DEV_SERVER=true electron ./electron",
    "start": "",
    "lint": "eslint . && tslint -p .",
    "lint-fix": "eslint --fix .",
    "document-wip": "jsdoc2md ./src/*.js > ./docs/doc.md"
  },
  "build": {
    "appId": "io.github.archifiltre",
    "files": [
      "electron/index.js",
      "electron/dist",
      "electron/build/icon.*",
      "package.json"
    ],
    "directories": {
      "output": "dist/${os}-${arch}"
    },
    "mac": {
      "icon": "./electron/build/icon.icns"
    },
    "win": {
      "icon": "./electron/build/icon.png"
    },
    "linux": {
      "category": "Office",
      "icon": "./electron/build/icon.png"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --list-different",
      "git add"
    ],
    "*.ts": [
      "tslint --fix",
      "prettier --list-different",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/archifiltre"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SocialGouv/archifiltre/issues"
  },
  "homepage": "https://github.com/SocialGouv/archifiltre#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@socialgouv/eslint-config-react": "0.10.0",
    "@socialgouv/tslint-config-recommended": "0.10.0",
    "@types/immutable": "3.8.7",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.144",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/react-redux": "7.1.4",
    "@types/redux": "3.6.0",
    "@types/redux-mock-store": "1.0.1",
    "awesome-typescript-loader": "5.2.1",
    "copy-webpack-plugin": "5.0.4",
    "cross-env": "6.0.3",
    "css-loader": "3.2.0",
    "deep-equal": "1.1.0",
    "electron": "6.0.12",
    "electron-builder": "21.2.0",
    "electron-react-devtools": "0.5.3",
    "eslint": "6.5.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "fast-check": "1.17.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.8",
    "image-webpack-loader": "4.6.0",
    "jest": "24.9.0",
    "jest-date-mock": "1.0.7",
    "jsdoc-to-markdown": "5.0.2",
    "lint-staged": "9.4.2",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "sass-loader": "8.0.0",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "svg-url-loader": "2.3.3",
    "ts-jest": "24.1.0",
    "tslint": "5.20.0",
    "typescript": "3.6.4",
    "url-loader": "1.1.2",
    "webpack": "4.41.1",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2",
    "worker-loader": "1.1.1"
  },
  "dependencies": {
    "@sentry/browser": "^5.4.0",
    "dateformat": "3.0.3",
    "docxtemplater": "^3.14.1",
    "electron-ga": "1.0.6",
    "eslint-plugin-import": "^2.18.2",
    "file-saver": "2.0.2",
    "foundation-sites": "^6.5.3",
    "immutable": "4.0.0-rc.12",
    "jquery": "^3.4.1",
    "js-md5": "0.7.3",
    "jszip": "3.2.2",
    "lodash": "^4.17.15",
    "md5-file": "^4.0.0",
    "pizzip": "^3.0.6",
    "prop-types": "^15.7.2",
    "raven": "^2.6.4",
    "react": "^16.9.5",
    "react-dom": "^16.9.5",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "riek": "1.1.0",
    "rxjs": "^6.5.3",
    "styled-components": "^4.3.2",
    "uuid": "3.3.3",
    "what-input": "^5.2.1",
    "xml": "1.0.1"
  }
}
