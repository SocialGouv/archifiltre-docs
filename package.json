{
  "name": "archifiltre",
  "version": "1.0.0",
  "description": "Explorer plus plus for archive",
  "main": "electron/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "yarn test --watch",
    "buildDev": "webpack --progress --mode development",
    "prepareProd": "webpack --progress --mode production",
    "buildProd": "yarn prepareProd && yarn win32 && yarn win64 && yarn mac && yarn linux",
    "devServer": "webpack-dev-server --mode development",
    "watchBuildDev": "webpack --watch --progress --mode development",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "win32": "electron-builder --windows portable --ia32",
    "win64": "electron-builder --windows portable --x64",
    "mac": "electron-builder --mac dmg",
    "linux": "electron-builder --linux AppImage",
    "postinstall": "electron-builder install-app-deps package.json",
    "electron": "electron ./electron",
    "electron-server": "cross-env DEV_SERVER=true electron ./electron",
    "start": "",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "document-wip": "jsdoc2md ./src/*.js > ./docs/doc.md"
  },
  "build": {
    "appId": "io.github.archifiltre",
    "files": [
      "electron/index.js",
      "electron/dist",
      "electron/build/icon.*",
      "package.json"
    ],
    "directories": {
      "output": "dist/${os}-${arch}"
    },
    "mac": {
      "icon": "./electron/build/icon.icns"
    },
    "win": {
      "icon": "./electron/build/icon.png"
    },
    "linux": {
      "category": "Office",
      "icon": "./electron/build/icon.png"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --list-different",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SocialGouv/archifiltre"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SocialGouv/archifiltre/issues"
  },
  "homepage": "https://github.com/SocialGouv/archifiltre#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@socialgouv/eslint-config-react": "^0.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "colors": "1.2.1",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.10",
    "electron": "^5.0.1",
    "electron-builder": "^20.39.0",
    "electron-react-devtools": "^0.5.3",
    "eslint": "^5.16.0",
    "file-loader": "2.0.0",
    "flow-bin": "0.66.0",
    "flow-webpack-plugin": "1.2.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "^3.0.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.8.0",
    "jest-date-mock": "^1.0.7",
    "jsdoc-to-markdown": "5.0.0",
    "lint-staged": "^9.1.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.20.3",
    "svg-url-loader": "^2.3.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "worker-loader": "1.1.1"
  },
  "dependencies": {
    "@sentry/browser": "^5.4.0",
    "core-js": "^3.1.4",
    "dateformat": "3.0.3",
    "electron-ga": "1.0.6",
    "file-saver": "2.0.2",
    "foundation-sites": "^6.5.3",
    "immutable": "4.0.0-rc.9",
    "jquery": "^3.4.1",
    "js-sha512": "0.8.0",
    "jszip": "3.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "riek": "1.1.0",
    "styled-components": "^4.3.2",
    "tar-fs": "1.16.3",
    "tar-stream": "1.6.2",
    "what-input": "^5.2.1",
    "xml": "1.0.1"
  }
}
