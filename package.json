{
  "name": "archifiltre-docs",
  "productName": "Docs par Archifiltre",
  "version": "4.1.0",
  "private": true,
  "description": "Visualisez et améliorez vos arborescences de fichiers !",
  "author": {
    "name": "Fabrique des ministères sociaux",
    "email": "archifiltre@fabrique.social.gouv.fr"
  },
  "license": "Apache-2.0",
  "main": "dist/main/main.js",
  "homepage": "https://archifiltre.fr/docs",
  "repository": "git@github.com:SocialGouv/archifiltre-docs.git",
  "bugs": {
    "url": "https://github.com/SocialGouv/archifiltre-docs/issues"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev": "electron-builder start",
    "test": "jest",
    "test:e2e": "playwright test --workers=1",
    "compile": "cross-env 'NODE_OPTIONS=\"--max-old-space-size=4096\"' electron-builder",
    "dist:win": "electron-builder --x64 --ia32 --win portable msi nsis",
    "dist:mac": "electron-builder --mac --x64 --arm64",
    "dist:mac-local": "CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac dmg",
    "dist:linux": "electron-builder --linux",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "lint:test": "eslint -c tests/.eslintrc.json 'tests/**/*.ts*'",
    "lint:fix-test": "eslint --fix -c tests/.eslintrc.json 'tests/**/*.ts*'",
    "lint:fix-all": "yarn lint:fix && yarn lint:fix-test",
    "devtools": "ts-node scripts/getReactDevtools.ts",
    "replaceForChannel": "node scripts/replaceForChannel.js",
    "generate-translation-csv": "node ./scripts/diff-translations.js",
    "import-translation-csv": "node ./scripts/translation-csv-to-json",
    "verify-translation-json": "node ./src/renderer/translations/verify-translation-json.js"
  },
  "electronWebpack": {
    "title": true,
    "main": {
      "webpackConfig": "webpack.main.config.js",
      "extraEntries": [
        "@/preload.ts"
      ]
    },
    "renderer": {
      "webpackConfig": "webpack.renderer.config.js"
    },
    "whiteListedModules": [
      "@devexpress/dx-react-chart-material-ui",
      "@devexpress/dx-react-chart",
      "@devexpress/dx-react-core",
      "@emeraldpay/hashicon-react",
      "@mui/icons-material",
      "@mui/lab",
      "@mui/material",
      "@mui/styles",
      "@sentry/electron",
      "angular-expressions",
      "csv-parse",
      "date-fns",
      "dateformat",
      "docxtemplater",
      "electron-store",
      "file-saver",
      "fswin",
      "i18next",
      "iconv-lite",
      "js-md5",
      "lodash",
      "md5-file",
      "mime-types",
      "pizzip",
      "posthog-js",
      "protobufjs",
      "react-dom",
      "react-i18next",
      "react-icons",
      "react-nice-dates",
      "react-redux",
      "react-toastify",
      "react",
      "redux-thunk",
      "redux",
      "rxjs",
      "source-map-support",
      "styled-components",
      "triple-beam",
      "uuid",
      "windows-1252",
      "winston-daily-rotate-file",
      "winston-transport-sentry-node",
      "winston-transport",
      "winston",
      "xlsx",
      "xml",
      "xstate",
      "jszip"
    ]
  },
  "//build.extraResources": "Static should be set again because it is overwritten",
  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "appId": "fr.gouv.social.fabrique.archifiltre.docs",
    "artifactName": "${name}-${os}-${arch}.${ext}",
    "afterSign": "scripts/notarize.js",
    "files": [
      "electron/build/icon*.*"
    ],
    "extraResources": [
      {
        "from": "dist/renderer/",
        "to": "workers/",
        "filter": [
          "**/*.fork.js*"
        ]
      },
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "preload.js"
        ]
      },
      {
        "from": "static",
        "to": "static",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer/lib/",
        "to": "lib/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "output": "electron/dist/${os}"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "./electron/build/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./electron/build/entitlements.mac.plist",
      "entitlementsInherit": "./electron/build/entitlements.mac.plist"
    },
    "dmg": {
      "title": "${name} ${version}"
    },
    "win": {
      "icon": "./electron/build/icon.png"
    },
    "portable": {
      "artifactName": "${name}-${os}-${arch}-portable.${ext}"
    },
    "msi": {
      "artifactName": "${name}-${os}-${arch}.${ext}"
    },
    "nsis": {
      "artifactName": "${name}-${os}-${arch}-setup.${ext}",
      "deleteAppDataOnUninstall": true,
      "language": 1036,
      "menuCategory": "Archifiltre",
      "installerIcon": "./electron/build/installerIcon.ico",
      "uninstallerIcon": "./electron/build/uninstallerIcon.ico",
      "include": "./electron/build/installer.nsh"
    },
    "linux": {
      "category": "Office",
      "icon": "./electron/build/icon.png",
      "target": [
        {
          "target": "AppImage"
        },
        {
          "target": "deb",
          "arch": "x64"
        }
      ]
    }
  },
  "engines": {
    "node": "20.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@electron/notarize": "^2.3.0",
    "@playwright/test": "^1.42.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@sentry/webpack-plugin": "^2.16.1",
    "@types/dateformat": "^5.0.2",
    "@types/deep-equal": "^1.0.4",
    "@types/file-saver": "^2.0.7",
    "@types/hidefile": "^3.0.3",
    "@types/jest": "29.5.12",
    "@types/js-md5": "^0.7.2",
    "@types/lodash": "4.17.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20",
    "@types/pizzip": "^3.0.5",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.24",
    "@types/react-redux": "7.1.33",
    "@types/redux-mock-store": "1.0.6",
    "@types/rimraf": "^3.0.2",
    "@types/styled-components": "^5.1.34",
    "@types/triple-beam": "^1.3.5",
    "@types/uuid": "^9.0.8",
    "@types/webpack": "5",
    "@types/xml": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "8.2.2",
    "copy-webpack-plugin": "12",
    "cross-env": "^7.0.3",
    "csv-stringify": "^6.4.6",
    "deep-equal": "2.2.3",
    "dotenv": "^16.4.5",
    "electron": "^29.2.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-import-resolver-webpack": "0.13.8",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-playwright": "^1.5.4",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "fast-check": "3.17.1",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.9",
    "node-loader": "^2",
    "postcss": "^8.4.38",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "redux-mock-store": "^1.5.4",
    "replace": "^1.2.2",
    "rimraf": "^5.0.5",
    "rmfr": "2.0.0",
    "sass": "^1.74.1",
    "sass-loader": "14",
    "semantic-release": "^23.0.7",
    "stdio-mock": "^1.2.0",
    "ts-jest": "29.1.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.3",
    "unzip-crx-3": "^0.2.0",
    "webpack": "5",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@devexpress/dx-react-chart": "^4.0.8",
    "@devexpress/dx-react-chart-material-ui": "^4.0.8",
    "@devexpress/dx-react-core": "^4.0.8",
    "@emeraldpay/hashicon-react": "^0.5.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.15",
    "@mui/system": "^5.15.15",
    "@mui/x-date-pickers": "^7.1.1",
    "@sentry/electron": "^4.21.0",
    "angular-expressions": "^1.2.1",
    "axios": "^1.6.8",
    "csv-parse": "^5.5.5",
    "date-fns": "^2.30.0",
    "dateformat": "5.0.3",
    "docxtemplater": "3.46.2",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "file-saver": "2.0.5",
    "fswin": "3.23.311",
    "i18next": "^23.10.1",
    "js-md5": "0.8.3",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "md5-file": "^5.0.0",
    "mime-types": "^2.1.35",
    "pizzip": "^3.1.6",
    "posthog-js": "^1.117.2",
    "posthog-node": "^4.0.0",
    "protobufjs": "^7.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-toastify": "^10.0.5",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "styled-components": "^6.1.8",
    "triple-beam": "^1.4.1",
    "ts-node": "^10.9.2",
    "uuid": "9.0.1",
    "windows-1252": "^3.0.4",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.7.0",
    "xlsx": "^0.18.5",
    "xml": "^1.0.1",
    "xstate": "^5.9.1"
  }
}
